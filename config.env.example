# SQL Builder Integration Test Configuration
# Copy this file to config.env and modify as needed

# MySQL Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=password
DB_NAME=test_sqltk
DB_TYPE=mysql

# PostgreSQL Database Configuration
PG_HOST=localhost
PG_PORT=5432
PG_USER=postgres
PG_PASSWORD=postgres
PG_DB=test_sqltk

# Docker Configuration
DOCKER_MYSQL_IMAGE=mysql:8.0
DOCKER_POSTGRES_IMAGE=postgres:15
DOCKER_MYSQL_PORT=3306
DOCKER_POSTGRES_PORT=5432

# Test Configuration
TEST_TIMEOUT=30s
TEST_RACE=false
TEST_COVERAGE=false
TEST_VERBOSE=true

# Go Configuration
GO=go
GOFLAGS=-mod=mod

# Database Connection Strings (auto-generated from above settings)
# MYSQL_DSN=root:password@tcp(localhost:3306)/
# POSTGRES_DSN=postgres://postgres:postgres@localhost:5432/postgres?sslmode=disable 